// prisma/schema.prisma

// Prisma Client generator
generator client {
  provider = "prisma-client-js"
  // âœ… Removed "output" so client is generated in node_modules/@prisma/client
}

// Database connection
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// Your Product model
model Product {
  id          Int    @id @default(autoincrement())
  title       String
  price       Int
  description String?
}
